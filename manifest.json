{
	"name": "autoSEEKr sideBar",
	"version": "11.42",
	"description": "Time to get a job... Why waste time manually applying like a CHUMP?  Annnnnd away we go...!",
	"icons": {
		"16": "images/gear16.png",
		"32": "images/gear32.png",
		"48": "images/gear48.png",
		"128": "images/gear128.png"
	},
	"sidebar_action": {
		"default_icon": {
			"16": "images/gear16.png",
			"32": "images/gear32.png"
		},
		"default_title": "autoSEEKr",
		"default_panel": "panel.html"
	},
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"https://seek.com.au/*"
			],
			"js": [
				"js/materialize.min.js",
				"js/mz-init.js",
				"js/content.js"
			],
			"css": [
				"css/materialize.min.css",
				"css/normalize.min.css",
				"css/content.css",
				"css/tooltips.css"
			],
			"all_frames": true
		}
	],
	"background": {
		"scripts": [
			"js/background.js"
		]
	},
	"permissions": [
		"https://*/",
		"tabs",
		"storage",
		"activeTab",
		"<all_urls>",
		"https://*.googleapis.com/*",
		"contextMenus"
	]
}
